{
    "name": "metadonors/odoo-rpc-client",
    "type": "library",
    "description": "A PHP Client for Odoo",
    "keywords": ["RPC", "XML", "XMLRPC", "Odoo"],
    "homepage": "https://github.com/metadonors/odoo-rpc-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrizio Arzeni",
            "email": "fabrizio.arzeni@metadonors.it"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "darkaonline/ripcord": "0.1.*"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "^7.3",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "OdooRPCClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}